<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.sangwoon.rang.CompareActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="상품 &lt;1>"
        android:id="@+id/left_productName"
        android:layout_alignParentTop="false"
        android:gravity="left"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="30dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="상품 &lt;2>"
        android:id="@+id/right_productName"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="30dp"
        android:gravity="right" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="나    트    륨"
        android:id="@+id/textView4"
        android:layout_marginTop="126dp"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/logo"
        android:layout_alignBottom="@+id/textView4"
        android:layout_marginTop="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="false"
        android:layout_marginBottom="20dp"
        android:layout_marginRight="180dp"
        android:layout_marginLeft="-30dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:src="@drawable/logo"
        android:layout_marginTop="30dp"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignParentTop="true"
        android:layout_marginRight="-30dp"
        android:layout_marginLeft="180dp"
        android:layout_marginBottom="20dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="지            방"
        android:id="@+id/textView5"
        android:layout_below="@+id/textView4"
        android:layout_alignLeft="@+id/textView4"
        android:layout_marginTop="10dp" />

    <TextView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="콜레스트롤"
        android:id="@+id/textView6"
        android:layout_alignStart="@+id/textView5"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textView5"
        android:layout_alignLeft="@+id/textView5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="단    백    질"
        android:id="@+id/textView7"
        android:layout_centerVertical="true"
        android:layout_alignStart="@+id/textView6"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textView6"
        android:layout_alignLeft="@+id/textView6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="탄 수  화 물"
        android:id="@+id/textView8"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="영양소 6"
        android:id="@+id/textView9"
        android:layout_marginTop="10dp"
        android:layout_below="@+id/textView8"
        android:layout_alignLeft="@+id/textView8"
        android:layout_alignStart="@+id/textView8" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="상품촬영"
        android:id="@+id/left_product_barcode"
        android:layout_marginTop="350dp"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="20dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="상품촬영"
        android:id="@+id/right_product_barcode"
        android:layout_marginTop="350dp"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="200dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Left_value1"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignLeft="@+id/left_product_barcode"
        android:layout_alignStart="@+id/left_product_barcode" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Left_value2"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/Left_value1"
        android:layout_alignStart="@+id/Left_value1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Left_value3"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignRight="@+id/Left_value2"
        android:layout_alignEnd="@+id/Left_value2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Left_value4"
        android:layout_above="@+id/textView8"
        android:layout_alignLeft="@+id/Left_value3"
        android:layout_alignStart="@+id/Left_value3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Left_value5"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignLeft="@+id/Left_value4"
        android:layout_alignStart="@+id/Left_value4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Left_value6"
        android:layout_alignLeft="@+id/Left_value5"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignStart="@+id/Left_value5" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Right_value1"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignRight="@+id/right_product_barcode"
        android:layout_alignEnd="@+id/right_product_barcode" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Right_value2"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignRight="@+id/Right_value1"
        android:layout_alignEnd="@+id/Right_value1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Right_value3"
        android:layout_alignTop="@+id/textView6"
        android:layout_alignRight="@+id/Right_value2"
        android:layout_alignEnd="@+id/Right_value2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Right_value4"
        android:layout_centerVertical="true"
        android:layout_alignTop="@+id/textView7"
        android:layout_alignRight="@+id/Right_value3"
        android:layout_alignEnd="@+id/Right_value3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Right_value5"
        android:layout_alignBottom="@+id/textView8"
        android:layout_alignRight="@+id/Right_value4"
        android:layout_alignEnd="@+id/Right_value4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/Right_value6"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignRight="@+id/Right_value5"
        android:layout_alignEnd="@+id/Right_value5" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:id="@+id/value01_arrow"
        android:layout_alignParentLeft="false"
        android:layout_alignTop="@+id/textView4"
        android:layout_toLeftOf="@+id/textView4" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:id="@+id/value02_arrow"
        android:layout_alignParentLeft="false"
        android:layout_alignTop="@+id/textView5"
        android:layout_toLeftOf="@+id/textView5" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:id="@+id/value03_arrow"
        android:layout_alignParentLeft="false"
        android:layout_alignTop="@+id/textView6"
        android:layout_toLeftOf="@+id/textView6" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:id="@+id/value04_arrow"
        android:layout_alignParentLeft="false"
        android:layout_alignTop="@+id/textView7"
        android:layout_toLeftOf="@+id/textView7" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:id="@+id/value05_arrow"
        android:layout_alignParentLeft="false"
        android:layout_alignTop="@+id/textView8"
        android:layout_toLeftOf="@+id/textView8" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:id="@+id/value06_arrow"
        android:layout_alignParentLeft="false"
        android:layout_alignTop="@+id/textView9"
        android:layout_toLeftOf="@+id/textView9" />

</RelativeLayout>
